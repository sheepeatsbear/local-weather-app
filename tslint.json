{
  "extends": [
    null,
    "tslint-etc",
    "tslint-config-prettier",
    "tslint-plugin-prettier",
    "tslint:recommended"
  ],
  "rules": {
    "prettier": true,
    "no-unused-declaration": true,
    "max-line-length": [true, 90],
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": [true, "never"]
  }
}
